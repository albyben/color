{
  "name": "color-ie",
  "version": "4.2.4",
  "description": "Color conversion and manipulation with CSS string support",
  "sideEffects": false,
  "keywords": [
    "color",
    "colour",
    "css"
  ],
  "authors": [
    "Josh Junon <josh@junon.me>",
    "Heather Arthur <fayearthur@gmail.com>",
    "Maxime Thirouin"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "license": "MIT",
  "repository": "Qix-/color",
  "xo": {
    "rules": {
      "no-cond-assign": 0,
      "new-cap": 0,
      "unicorn/prefer-module": 0,
      "no-mixed-operators": 0,
      "complexity": 0,
      "unicorn/numeric-separators-style": 0
    }
  },
  "files": [
    "LICENSE",
    "lib"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "mocha",
    "build": "swc ./index.js -d lib"
  },
  "engines": {
    "node": ">=12.5.0"
  },
  "dependencies": {
    "color-convert-ie": "2.0.2",
    "color-string": "^1.9.0"
  },
  "devDependencies": {
    "@swc/cli": "0.3.10",
    "@swc/core": "1.4.2",
    "mocha": "9.0.2",
    "xo": "0.42.0"
  }
}
